{
  "name": "originlytics",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:python": "python3 python/test_seo_analyzer.py",
    "test:all": "npm run test:python && npm run test",
    "server": "node src/server.js",
    "start": "node server.js",
    "mud:server": "node mud-games/server/mud-server.js",
    "mud:dev": "nodemon mud-games/server/mud-server.js",
    "playwright:install": "playwright install chromium",
    "python:install": "pip install -r requirements.txt",
    "setup": "npm run playwright:install && npm run python:install"
  },
